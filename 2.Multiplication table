import java.io.*;
import java.util.*;
class Main
{
public static void main (String[] args)
{
    int i;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the value of n");
    int n=sc.nextInt();
    for(i=1;i<=10;i++)
    {
        int N=n*i;
        System.out.println(n+"*"+i+"="+N);
    }
}
}
